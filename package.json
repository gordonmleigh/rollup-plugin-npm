{
  "name": "@gordonmleigh/rollup-plugin-npm",
  "version": "0.1.1",
  "type": "module",
  "author": {
    "name": "Gordon Leigh"
  },
  "description": "A Rollup plugin to install npm packages into the output",
  "keywords": [
    "rollup-plugin",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gordonmleigh/rollup-plugin-npm"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint",
    "clean": "rm -rf lib/ *.tsbuildinfo",
    "compile": "tsc",
    "lint": "eslint src/ --ext=ts"
  },
  "exports": {
    ".": {
      "types": "./lib/plugin.d.ts",
      "default": "./lib/plugin.js"
    }
  },
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "rollup": "^4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "memoize": "^10.0.0",
    "tempy": "^3.1.0"
  }
}
